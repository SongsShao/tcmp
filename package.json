{
  "private": true,
  "scripts": {
    "start": "UMI_UI=1 umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "generate": "node ./config/publish/generate-publish.js",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "@ant-design/pro-form": "^1.16.0",
    "@ant-design/pro-layout": "^6.5.0",
    "@types/mockjs": "^1.0.4",
    "antd": "^4.16.13",
    "child_process": "^1.0.2",
    "classnames": "^2.2.6",
    "inquirer": "^8.2.0",
    "mockjs": "^1.1.0",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "react": "17.x",
    "react-dom": "17.x",
    "react-router": "^4.3.1",
    "signale": "^1.4.0",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "^1.8.24",
    "@umijs/preset-ui": "^2.2.9",
    "@umijs/test": "^3.5.20",
    "lint-staged": "^11.2.3",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
